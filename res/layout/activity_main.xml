<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context="com.example.cs3270a2.MainActivity"
    tools:ignore="MergeRootFrame" >

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            
                <EditText
        android:id="@+id/editText1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="Android Logos"
        android:textColor="#AA00AA"
        android:textSize="50sp" >

        <requestFocus />


    </EditText>
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
                
                    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="138dp" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="123dp"
                android:layout_weight="1.00"
                android:gravity="center"
                android:src="@drawable/android_logo_10" />
            
            

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="125dp"
                android:layout_weight="0.61"
                android:gravity="center"
                android:src="@drawable/android_logo_16" />
            
            
            
        </LinearLayout>
            
        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
                    
              <TextView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:singleLine="true"
            android:text="   Ice Cream          Cool Guy"
            android:textColor="#AA00AA"
            android:textSize="30sp"
            android:textStyle="bold" />
              
  
        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
              
            
                                <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="138dp" >

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="123dp"
                android:layout_weight="1.00"
                android:gravity="center"
                android:src="@drawable/android_logo_12" />
            
            

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="125dp"
                android:layout_weight="0.61"
                android:gravity="center"
                android:src="@drawable/android_logo_18" />
            
            
            
        </LinearLayout>
        </TableRow>
        
                <TableRow
            android:id="@+id/tableRow8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
                    
              <TextView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:singleLine="true"
            android:text="    Frazzled          Circle Guy"
            android:textColor="#AA00AA"
            android:textSize="30sp"
            android:textStyle="bold" />
              
  
        </TableRow>
        
    </TableLayout>

</FrameLayout>

